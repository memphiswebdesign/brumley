<script>
( function( $ ) {

	"use strict";

  $(".tilt-card").tilt({
    maxTilt: 7,
    perspective: 800,
    easing: "cubic-bezier(.03,.98,.52,.99)",
    speed: 1000,
    glare: false,
    maxGlare: 0.3,
    scale: 1.05
  });
  
}( jQuery ) );
</script>

